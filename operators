<script>

    var a = 10;
    var b = 20;
    var name = "ali haider";
    var pass = "ali123";

    if (a == b) {
        console.log("yes");
    }
    else
    {
        console.log('no');
    }


    if (a > 5) {
        console.log("condition is true");
    }
    else
    {
        console.log("Condition is false");
    }

    
    if (b < 20) {
        console.log("condition is true");
    }
    else
    {
        console.log("Condition is false");
    }

    if ((a > 5) && (b < 20)) {
        console.log("condition is true");
    }
    else
    {
        console.log("Condition is false");
    }


    if ((name == "ali haider") && (pass == "ali123")) {
        console.log("Yes you are Ali haider");
    }
    else
    {
        console.log("No you are not Ali haider");
    }

if (name == "ali haider") {
    console.log("Access granted");
    
} else {
    console.log("Acess Denied");
}

</script>

